     #include <gsl/gsl_multifit.h>
     #include <gsl/gsl_blas.h>

     static int
     hilbert_matrix(gsl_matrix * m)
     {
       const size_t N = m->size1;
       const size_t M = m->size2;
       size_t i, j;

       for (i = 0; i < N; i++)
         {
           for (j = 0; j < M; j++)
             {
               gsl_matrix_set(m, i, j, 1.0/(i+j+1.0));
             }
         }

       return GSL_SUCCESS;
     }

